@using Proyecto.Clientes.MVC.Entidades
@model List<Cliente>
<h2 style="text-align:center">Todos los vendedores</h2>

<table class="table">
    <tr>
        <td>ID</td>
        <td>Nombre</td>
        <td>Apellido</td>
        <td>Antiguedad</td>
    </tr>
    @foreach (Cliente c in Model)
    {
        <tr>
            <td>@c.Id</td>
            <td>@c.Nombre</td>
            <td>@c.Apellido</td>
            <td>@c.Antiguedad</td>
        </tr>
    }
</table>

